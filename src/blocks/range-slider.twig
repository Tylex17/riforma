<div class="range-slider-wr">
	<div class="range-slider-labels">
		<div class="range-slider-labels__item">
			от <span class="range-slider-labels_from">12</span>
		</div>
		<div class="range-slider-labels__item">
			до <span class="range-slider-labels_to">12</span>
		</div>
	</div>
	<div class="range-slider" data-field-name="{{fieldName}}" data-type="{{type}}" data-min="{{min}}" data-max="{{max}}" data-from="{{from}}" data-to="{{to}}"></div>
	<input type="hidden" class="range-input" name="{{fieldName}}-min">
	<input type="hidden" class="range-input" name="{{fieldName}}-max">
</div>