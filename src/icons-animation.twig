						<g data-name="Group 1312">
							<path class="modular-part-1" data-name="Path 5236" d="M5.02 16.966l-4.1 1.427v10.835h44.848v-12.36H16.324L9.905.001.005 3.77z" fill="#613175"/>
							<g data-name="Group 1246">
								<g data-name="Group 1003" fill="#613175">
									<path class="modular-part-2" data-name="Path 4781" d="M55.084 17v12.36h21.915V17z"/>
									<path class="modular-part-3" data-name="Path 4782" d="M85.084 17v12.36h21.915V17z"/>
								</g>
							</g>
						</g>
					</svg>
				</div>
				<div class="animations__desc">
					Модульные диваны
				</div>
			</div>
			<div class="animations__item">
				<div class="animations__icon" data-sofa-type="complects" onmouseenter="o2.animations.sofaIcon.enter(this)" onmouseleave="o2.animations.sofaIcon.leave(this)">
					<svg xmlns="http://www.w3.org/2000/svg" width="123.935" height="29.228" viewBox="0 0 123.935 29.228">
						<g data-name="Group 1247">
							<g data-name="Group 783" fill="#613175">
								<path class="complects-part-1" data-name="Path 4372" d="M5.02 16.965l-4.1 1.427v10.835h44.848v-12.36H16.324L9.905 0l-9.9 3.769zM94.02 16.965l-4.1 1.427v10.835h34.015v-12.36h-18.611L98.905 0l-9.9 3.769z"/>
								<g class="complects-part-2" data-name="Group 66" stroke="#613175" stroke-width="3">
									<path data-name="Rectangle 165" d="M62 15.039v-1.813h11.333v1.813z"/>
									<path data-name="Rectangle 166" d="M66.76 8.466h1.813v11.333H66.76z"/>
								</g>
							</g>
						</g>
					</svg>
				</div>
				<div class="animations__desc">
					Комплекты
				</div>
			</div>
			<div class="animations__item">
				<div class="animations__icon" data-sofa-type="sale" onmouseenter="o2.animations.sofaIcon.enter(this)" onmouseleave="o2.animations.sofaIcon.leave(this)">
					<svg role="img" class="sale-logo">
